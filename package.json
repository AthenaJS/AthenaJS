{
    "name": "athenajs",
    "version": "0.0.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/AthenaJS/athenajs.git"
    },
    "description": "AthenaJS is a framework for building 2D games using HTML5",
    "author": "Nicolas Ramz",
    "license": "MIT",
    "main": "dist/athena.js",
    "keywords": [
        "html5",
        "canvas",
        "spritesheet",
        "game",
        "map",
        "tile"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack --watch",
        "build": "webpack --config webpack.production.config.js",
        "build-docs": "documentation build js/** --watch --config docs/documentation.yml --github --shallow -f html -o docs"
    },
    "devDependencies": {
        "babel-core": "6.24.1",
        "babel-loader": "6.4.1",
        "babel-preset-es2015": "6.24.1",
        "circular-dependency-plugin": "2.0.0",
        "documentation": "4.0.0-beta.19",
        "exports-loader": "0.6.4",
        "imports-loader": "0.7.1",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "webpack": "2.3.3",
        "webpack-notifier": "1.5.0"
    },
    "dependencies": {
        "es6-promise": "4.1.0",
        "whatwg-fetch": "2.0.3",
        "web-audio-daw": "2.3.1"
    }
}
