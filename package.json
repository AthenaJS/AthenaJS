{
    "name": "athenajs",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/AthenaJS/athenajs.git"
    },
    "description": "AthenaJS is a framework for building 2D games using HTML5",
    "author": "Nicolas Ramz",
    "license": "MIT",
    "main": "./dist/athena.js",
    "keywords": [
        "html5",
        "canvas",
        "spritesheet",
        "game",
        "map",
        "tile"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "start": "webpack --watch",
        "build": "webpack --config webpack.production.config.js",
        "build-docs": "docma -c ./docma.json",
        "check": "tsc --allowJs --checkJs ./js/athena-module.js"
    },
    "devDependencies": {
        "@babel/core": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.0.2",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "circular-dependency-plugin": "2.0.0",
        "docma": "1.5.2",
        "exports-loader": "^0.7.0",
        "imports-loader": "^0.8.0",
        "jest-cli": "^23.6.0",
        "jsdoc": "^3.5.5",
        "node-notifier": "5.1.2",
        "typescript": "^2.5.3",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.1",
        "webpack-dev-server": "^3.1.8",
        "webpack-notifier": "1.5.0"
    },
    "dependencies": {
        "es6-promise": "4.1.0",
        "web-audio-daw": "2.3.1",
        "whatwg-fetch": "2.0.3"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    },
    "resolutions": {
        "babel-core": "7.0.0-bridge.0"
    }
}
