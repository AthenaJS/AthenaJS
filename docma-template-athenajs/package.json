{
    "name": "docma-template-athenajs",
    "version": "1.0.0",
    "description": "Docma Template for AthenaJS",
    "repository": "athenajs/docma-template-athenajs",
    "license": "MIT",
    "author": "Nicolas Ramz <nicolas.ramz@gmail.com>",
    "files": [
        "template/",
        "index.js",
        "LICENSE"
    ],
    "scripts": {
        "less": "lessc ./src/less/styles.less ./template/css/styles.css --clean-css=\"--s1 --advanced\"",
        "uglify": "uglifyjs ./src/js/app.js -o ./template/js/app.min.js --comments",
        "build": "npm run less && npm run uglify"
    },
    "keywords": [
        "docma",
        "template",
        "athenajs"
    ],
    "peerDependencies": {
        "docma": ">=2.1.0"
    },
    "devDependencies": {
        "less": "^3.9.0",
        "less-plugin-clean-css": "^1.5.1",
        "uglify-js": "^3.4.9"
    },
    "dependencies": {
        "docma": "^2.1.0"
    }
}
