{
    "name": "Docma AthenaJS Template",
    "author": "Nicolas Ramz (nicolas.ramz@gmail.com)",
    "version": "0.1.0",
    "license": "MIT",
    // Main HTML file
    "main": "index.html",
    // Template default options.
    "options": {
        "title": "",
        "sidebar": true,
        "collapsed": false,
        "outline": "tree",
        "badges": true,
        "symbolMeta": false,
        "search": true,
        "navbar": true,
        "collapseSymbols": true,
        "collapseDefinition": true,
        "propertiesLast": true,        
        "navItems": []
    },
    // files to be compiled
    "compile": {
        // JS files to be minified
        "js/app.js": "js/app.min.js",
        // Less files to be compiled
        "less/styles.less": "css/styles.css"
    },
    // Ignored files/dirs
    "ignore": [
        "js/app.js",
        "less/**",
        "*.md",
        "LICENSE"
    ]
}
